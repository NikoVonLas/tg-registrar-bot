{
  "name": "@bot-platform/event-registration-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc -b && tsc-alias"
  },
  "keywords": [
    "telegram",
    "bot",
    "event",
    "registration"
  ],
  "dependencies": {
    "@nikovonlas/bot-sdk": "^1.0.2",
    "@types/pdfkit": "^0.17.5",
    "@types/qrcode": "^1.5.6",
    "grammy": "^1.23.0",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.3.3"
  },
  "bot": {
    "type": "telegram",
    "env": {
      "required": [
        "BOT_TOKEN"
      ],
      "optional": [
        "LANGUAGE",
        "DEBUG"
      ]
    },
    "os-packages": []
  }
}